{
  "name": "@pleisto/active-support",
  "version": "0.2.1",
  "description": "A TypeScript utility library designed to optimize for programmer happiness.",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pleisto/active-support.git"
  },
  "bugs": {
    "url": "https://github.com/pleisto/active-support"
  },
  "module": "commonjs",
  "main": "./dist/index.ts",
  "types": "./dist/index.ts",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    }
  },
  "volta": {
    "node": "20.14.0"
  },
  "engines": {
    "node": "20"
  },
  "scripts": {
    "lint": "run lint:eslint && run lint:type",
    "lint:eslint": "TIMING=1 run eslint --cache --ext ts,tsx src",
    "lint:fix": "TIMING=1 run eslint --cache --ext ts,tsx src --fix",
    "lint:type": "run tsc",
    "build": "rm -rf dist && run tsc --build ./tsconfig.build.json",
    "test": "run vitest run --coverage",
    "test:watch": "run vitest --watch",
    "test:bench": "run -vitest bench",
    "tsdoc": "run typedoc --options typedoc.json --out ./docs/api ./src/index.ts"
  },
  "sideEffects": false,
  "dependencies": {
    "@lukeed/uuid": "^2.0.1",
    "fast-deep-equal": "^3.1.3",
    "lodash": "^4.17.21",
    "moize": "^6.1.6",
    "ms": "^3.0.0-canary.1",
    "neverthrow": "^6.2.2",
    "performant-array-to-tree": "^1.11.0",
    "radash": "^12.1.0",
    "ts-pattern": "^5.2.0",
    "type-fest": "^4.20.0"
  },
  "devDependencies": {
    "@pleisto/eslint-plugin": "1.5.3",
    "@types/lodash": "^4.17.5",
    "@types/node": "20.14.2",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "8.57.0",
    "prettier": "^3.3.2",
    "tslib": "^2.6.3",
    "typedoc": "^0.25.13",
    "typedoc-plugin-inline-sources": "^1.0.2",
    "typescript": "5.4.5",
    "vite": "5.2.13",
    "vitest": "1.6.0"
  },
  "packageManager": "yarn@4.3.0"
}
